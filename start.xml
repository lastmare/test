<?xml version="1.0" encoding="UTF-8"?>
<MOML>
    <THEMES>
        <THEME id ="style" style="round" padding="10" margine="5"/>
    <UILAYOUT portrait="320,460" landscape="320,460">
        <WINDOW layout="0,0,320,460" defaultImg="#ffffff">
            <LABEL layout="10,10,auto,auto" text="Hello MOML" />
           <!-- <WEBVIEW layout ="10,10,300,300" src="http://naver.com" />-->
           <CAMERA id= "cam" layout="10,60,300,300" />
           <WINDOW layout="10,380,300,auto" align="linear">
               <STYLEBUTTON themeId="style" text="open" onClick="cam.open"/>
                   <STYLEBUTTON themeId="style" text="close" onClick="cam.close"/>
                       <STYLEBUTTON themeId="style" text="front" onClick="cam.type='front'"/>
                       <STYLEBUTTON themeId="style" text="back" onClick="cam.type='back'"/>
                       <STYLEBUTTON themeId="style" text="Tack" onClick="cam.takePicture"/>
        </WINDOW>
        </WINDOW>
    </UILAYOUT>
</MOML>
